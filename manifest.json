{
	"manifest_version": 2,
	"name": "书签快速添加",
	"version": "1.0.1",
	"description": "快速将书签添加到系统 http://mybookmark.cn/ 中",
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "点击将当前页快速添加书签列表",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"js/externe/jquery-1.8.3.js",
				"js/content-script.js"
			],
			"css": [

			],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"http://*/*",
		"https://*/*"
	],
	"web_accessible_resources": [
		"js/inject.js"
	],
	"homepage_url": "http://mybookmark.cn/",
	"omnibox": {
		"keyword": "bookmark"
	}
}